{
    "manifest_version": 2,
    "name": "TrustBlock",
    "version": "0.1",
    "background": {
        "scripts": ["./scripts/background.js"]
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ],
    "browser_action":{
        "default_icon": "./assets/icon.png"
    }
}
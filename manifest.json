{
    "manifest_version": 2,
    "name": "TrustBLK",
    "version": "0.1",
    "description": "Trustworthy and Fair Ad-Blocker",
    "background": {
        "scripts": ["./scripts/background.js"]
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ],
    "browser_action":{
        "default_icon": "./assets/logo.png",
        "default_popup": "./html/popup.html"
    }
}